{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": false,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "paths": {
      "dummy/tests/*": [
        "tests/*"
      ],
      "dummy/*": [
        "tests/dummy/app/*",
        "app/*"
      ]
    }
  },
  "include": [
    "tests"
  ]
}